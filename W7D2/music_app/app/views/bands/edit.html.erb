<form action="<%=band_url(@band)%>" method="POST">
    <input type= "hidden" name= "_method" value="PATCH">
    <input type= "hidden" name="authenticity_token" value = <%= form_authenticity_token %>>
    <input type= "text" name="band[band_name]" value= "<%= @band.band_name%>"><br>
    <input type= "text" name="band[formation_year]" value="<%= @band.formation_year%>"><br>
    <input type= "text" name="band[location]" value="<%= @band.location%>"><br>
    <input type= "submit" value="Submit changes">
</form>